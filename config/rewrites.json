{
    "developer.rackspace.com": [
        {
            "from": "^\/docs\/user-guides\/?$",
            "to": "/docs/user-guides/infrastructure/",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Force trailing slash",
            "from": "\\/([a-zA-Z0-9%_-]*?[^/])$",
            "to": "/$1/",
            "rewrite": false,
            "status": 301
        }
    ]
}
